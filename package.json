{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start:app": "cd ./app && BROWSER=none yarn start",
    "start:electron": "electron .",
    "start": "concurrently \"npm:start:app\" \"wait-on http://localhost:3000 && npm run start:electron\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "concurrently": "^5.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "wait-on": "^4.0.1"
  },
  "devDependencies": {
    "electron": "^8.2.0"
  }
}