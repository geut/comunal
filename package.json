{
  "name": "@geut/comunal",
  "version": "0.0.1",
  "description": "Template to create modules following the Geut path",
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
